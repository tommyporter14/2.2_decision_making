import java.util.Scanner;//import scanner
public class decision_making {

	public static void main(String[] args) {

		int userInput = 0;
		
		Scanner scnr = new Scanner(System.in);//assigning scnr as user input
		
		System.out.println("Enter a number between 1 and 100: ");//ask user to respond heads or tails
		userInput = scnr.nextInt();
		
		if (userInput % 2 != 0)
		{
			System.out.println(userInput + " and Odd");
		}
		else if (userInput % 2 == 0 && userInput > 25 && userInput <= 2)
		{
			System.out.println("Even and less than 25");
		}
		else if (userInput % 2 == 0 && userInput >= 26 && userInput <= 60)
		{
			System.out.println("Even");
		}
		else if (userInput % 2 == 0 && userInput > 60)
		{
			System.out.println(userInput + " and Even");
		}
		else if(userInput % 2 != 0 && userInput > 60)
		{
			System.out.println(userInput + " Odd and over 60");
		}
		else 
		{
			
		}
		scnr.close();//scanner close
	}

}
